## Frontend (React) environment variables

# Toggle admin interface (used in code to switch AdminApp)
REACT_APP_ADMIN=false

# Backend API base URL (some screens use hardcoded localhost; this is a helper)
REACT_APP_API_BASE_URL=http://localhost:5010

# Optional feature flags
REACT_APP_ENABLE_ANALYTICS=false

# Gemini (frontend feature flags / config)
REACT_APP_GEMINI_MODEL=gemini-1.5-flash
REACT_APP_GEMINI_ENABLED=false


REACT_APP_API_BASE=http://localhost:5010
REACT_APP_MAX_IMAGE_MB=25
REACT_APP_MAX_VIDEO_MB=200
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp
REACT_APP_ALLOWED_VIDEO_TYPES=video/mp4,video/webm,video/quicktime

# Optional analytics/monitoring (public)
# REACT_APP_GA_MEASUREMENT_ID=
# REACT_APP_SENTRY_DSN=